{
  "name": "tmatejicek/jquery-fileupload",
  "description": "Nette UploadControl rozšířené o blueimp jQuery FileUpload.",
  "homepage": "https://github.com/JZechy/jQuery-FileUpload",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan 'Zechy' Zechovský",
      "email": "email@zechy.cz",
      "homepage": "http://zechy.cz"
    }
  ],
  "require": {
    "php": "7.4.*",
    "ext-json": "*",
    "latte/latte": "^2",
    "nette/application": "^3",
    "nette/caching": "^3",
    "nette/di": "^3",
    "nette/forms": "^3",
    "nette/http": "^3",
    "nette/utils": "^3"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-nette": "^1.2",
    "phpstan/phpstan-strict-rules": "^1.5",
    "tracy/tracy": "^2"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  }
}
